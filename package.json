{
  "name": "@marp-team/marpit",
  "version": "1.5.2",
  "description": "The skinny framework for creating slide deck from Markdown",
  "license": "MIT",
  "author": {
    "name": "Marp team",
    "url": "https://github.com/marp-team"
  },
  "homepage": "https://marpit.marp.app/",
  "contributors": [
    {
      "name": "Yuki Hattori",
      "url": "https://github.com/yhatt"
    }
  ],
  "keywords": [
    "marp",
    "markdown",
    "parser",
    "slide",
    "deck",
    "presentation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/marp-team/marpit"
  },
  "engines": {
    "node": ">=8"
  },
  "main": "lib/index.js",
  "types": "index.d.ts",
  "files": [
    "lib/",
    "index.d.ts",
    "plugin.js"
  ],
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "build": "yarn -s clean && babel src --out-dir lib",
    "check:audit": "yarn audit",
    "check:format": "yarn -s run format -c",
    "clean": "rimraf lib",
    "docs": "run-p docsify docsify:style:watch",
    "docsify": "browser-sync start -s docs -f \"docs/**/*\" --single --no-open",
    "docsify:style": "node ./docsify/build.js",
    "docsify:style:watch": "nodemon -q -e .scss,.js -w ./docsify ./docsify/build.js",
    "format": "prettier \"**/*.{css,html,js,json,md,scss,ts,yaml,yml}\"",
    "jsdoc": "rimraf jsdoc && jsdoc src -c .jsdoc.json",
    "lint:js": "eslint .",
    "lint:css": "stylelint \"./**/*.{css,scss}\"",
    "prepack": "npm-run-all --parallel check:* lint:* test:coverage --sequential build",
    "preversion": "run-p check:* lint:* test:coverage",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "version": "curl https://raw.githubusercontent.com/marp-team/actions/v1/lib/scripts/version.js | node && git add -A CHANGELOG.md",
    "watch": "babel src --out-dir lib -w --verbose"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-proposal-private-methods": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "browser-sync": "^2.26.7",
    "cheerio": "^1.0.0-rc.3",
    "codecov": "^3.6.5",
    "cssnano": "^4.1.10",
    "dedent": "^0.7.0",
    "docsify-themeable": "^0.8.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "jest": "^25.5.4",
    "jest-junit": "^10.0.0",
    "jsdoc": "^3.6.4",
    "minami": "^1.2.3",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sass": "1.26.5",
    "stylelint": "^13.3.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.17.1"
  },
  "dependencies": {
    "color-string": "^1.5.3",
    "js-yaml": "^3.13.0",
    "lodash.kebabcase": "^4.1.1",
    "markdown-it": "^10.0.0",
    "markdown-it-front-matter": "^0.2.1",
    "postcss": "^7.0.28"
  },
  "publishConfig": {
    "access": "public"
  }
}
